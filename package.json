{
  "name": "darca",
  "version": "0.2.0",
  "description": "Dependency Assistant for Really Complex Applications",
  "main": "dist/index.js",
  "bin": {
    "darca": "dist/index.js"
  },
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "format": "prettier --write '**/*.{ts,js,json,less,md,html}'"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "keywords": [
    "npm",
    "dev",
    "manager",
    "cli"
  ],
  "author": "Stanislav Kubik <skubik3@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "rimraf": "^5.0.1",
    "verdaccio": "^5.26.2"
  },
  "devDependencies": {
    "@types/node": "^16.18.48",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
