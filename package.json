{
  "name": "darca",
  "version": "0.0.5",
  "description": "Dependency Assistent for Really Complex Applications",
  "main": "src/index.js",
  "bin": "src/index.js",
  "files": [
    "src/**/*.js"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,json,less,md,html}'"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
