  "keywords": [
    "npm",
    "dev",
    "manager"
  ],
  "author": "Stanislav Kubik <skubik3@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "rimraf": "^5.0.1",
    "verdaccio": "^5.26.2"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3"
  }
}
